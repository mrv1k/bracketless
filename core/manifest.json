{
  "name": "Bracketless",
  "version": "0.5.0",
  "description": "Collapse text between the brackets",
  "homepage_url": "https://github.com/mrv1k/bracketless#readme",
  "author": "Viktor Khotimchenko",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "optional_permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "src/bg/eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/bracketless.png",
    "default_title": "Load bracketless for current page"
  },
  "options_ui": {
    "page": "src/options/options.html",
    "chrome_style": true
  },
  "manifest_version": 2
}
